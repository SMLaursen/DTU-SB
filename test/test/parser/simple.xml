<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model id="unnamed1">
    <listOfCompartments>
      <compartment id="compartment" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="R" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="P" compartment="compartment" hasOnlySubstanceUnits="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="reaction" reversible="false">
        <listOfReactants>
          <speciesReference species="R"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <cn> 0.1 </cn>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay" reversible="false">
        <listOfReactants>
          <speciesReference species="R"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <times/>
                <cn>0.001</cn>
                <ci>P</ci>
              </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
    <listOfInitialAssignments>
        <initialAssignment symbol="P">
             <math xmlns="http://www.w3.org/1998/Math/MathML">
                 <cn>1</cn>
             </math>
         </initialAssignment>
    </listOfInitialAssignments>
  </model>
</sbml>
