<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model>
    <listOfCompartments>
      <compartment id="compartment" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="CI" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="CII" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="S_1" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="S_2" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="P_RE" compartment="compartment" initialAmount="1" hasOnlySubstanceUnits="true"/>
      <species id="P_R" compartment="compartment" initialAmount="1" hasOnlySubstanceUnits="true"/>
      <species id="RNAP" compartment="compartment" initialAmount="30" hasOnlySubstanceUnits="true"/>
      <species id="CI_2" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="S_3" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="S_4" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="decay_CI" reversible="false">
        <listOfReactants>
          <speciesReference species="CI"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.0075 </cn>
              <ci> CI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay_CII" reversible="false">
        <listOfReactants>
          <speciesReference species="CII"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.0075 </cn>
              <ci> CII </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="tl_P_RE_CI" reversible="false">
        <listOfProducts>
          <speciesReference species="CI" stoichiometry="10"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="S_1"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="e-notation"> 4 <sep/> -5 </cn>
              <ci> S_1 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="tl_P_R_CII" reversible="false">
        <listOfProducts>
          <speciesReference species="CII" stoichiometry="10"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="S_2"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.014 </cn>
              <ci> S_2 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Open_P_RE" reversible="false">
        <listOfReactants>
          <speciesReference species="RNAP"/>
          <speciesReference species="P_RE"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S_1"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.01 </cn>
              <ci> P_RE </ci>
              <ci> RNAP </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Open_P_R" reversible="false">
        <listOfReactants>
          <speciesReference species="RNAP"/>
          <speciesReference species="P_R"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S_2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.69422 </cn>
              <ci> P_R </ci>
              <ci> RNAP </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Close_P_RE" reversible="false">
        <listOfReactants>
          <speciesReference species="S_1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="RNAP"/>
          <speciesReference species="P_RE"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> S_1 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Close_P_R" reversible="false">
        <listOfReactants>
          <speciesReference species="S_2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P_R"/>
          <speciesReference species="RNAP"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> S_2 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Dimeriz_2" reversible="false">
        <listOfReactants>
          <speciesReference species="CI" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="CI_2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.1 </cn>
              <apply>
                <power/>
                <ci> CI </ci>
                <cn type="integer"> 2 </cn>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Dimeriz" reversible="false">
        <listOfReactants>
          <speciesReference species="CI_2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="CI" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> CI_2 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Rep_P_R" reversible="false">
        <listOfReactants>
          <speciesReference species="P_R"/>
          <speciesReference species="CI_2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S_3"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.2165 </cn>
              <ci> P_R </ci>
              <ci> CI_2 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="S_3_unbind" reversible="false">
        <listOfReactants>
          <speciesReference species="S_3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="CI_2"/>
          <speciesReference species="P_R"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> S_3 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Act_P_RE" reversible="false">
        <listOfReactants>
          <speciesReference species="P_RE"/>
          <speciesReference species="RNAP"/>
          <speciesReference species="CII"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S_4"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.00161 </cn>
              <ci> P_RE </ci>
              <ci> CII </ci>
              <ci> RNAP </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="tl_S_4_CI" reversible="false">
        <listOfProducts>
          <speciesReference species="CI" stoichiometry="10"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="S_4"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.015 </cn>
              <ci> S_4 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="S_4_unbind" reversible="false">
        <listOfReactants>
          <speciesReference species="S_4"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P_RE"/>
          <speciesReference species="RNAP"/>
          <speciesReference species="CII"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> S_4 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
