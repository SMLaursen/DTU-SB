<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model>
    <listOfCompartments>
      <compartment id="compartment" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="mRNA" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="lacI" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="Plac" compartment="compartment" initialAmount="1" hasOnlySubstanceUnits="true"/>
      <species id="Plac_lacI" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="transcription" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="mRNA"/>
          <speciesReference species="Plac"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <cn type="integer"> 10 </cn>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay_mRNA" reversible="false">
        <listOfReactants>
          <speciesReference species="mRNA"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <exp/>
              <apply>
                <times/>
                <cn> 0.54 </cn>
                <ci> mRNA </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="translation" reversible="false">
        <listOfReactants>
          <speciesReference species="mRNA"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lacI"/>
          <speciesReference species="mRNA"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 90 </cn>
              <ci> mRNA </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay_lacI" reversible="false">
        <listOfReactants>
          <speciesReference species="lacI"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <cn type="integer"> 1080 </cn>
              <apply>
                <plus/>
                <cn> 0.1 </cn>
                <apply>
                  <times/>
                  <cn type="integer"> 4 </cn>
                  <ci> lacI </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="regulation" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac"/>
          <speciesReference species="lacI"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Plac_lacI"/>
          <speciesReference species="lacI"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 3 </cn>
              <cn> 1.8 </cn>
              <apply>
                <plus/>
                <apply>
                  <times/>
                  <apply>
                    <minus/>
                    <cn> 0.2 </cn>
                    <cn> 0.01 </cn>
                  </apply>
                  <apply>
                    <divide/>
                    <cn type="integer"> 1 </cn>
                    <apply>
                      <plus/>
                      <cn type="integer"> 1 </cn>
                      <apply>
                        <power/>
                        <apply>
                          <divide/>
                          <cn> 2 </cn>
                          <cn> 0.0035 </cn>
                        </apply>
                        <cn type="integer"> 2 </cn>
                      </apply>
                    </apply>
                  </apply>
                </apply>
                <cn> 0.01 </cn>
              </apply>
              <ci> Plac </ci>
              <ci> lacI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="unbinds" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac_lacI"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Plac"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.006 </cn>
              <ci> Plac_lacI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
