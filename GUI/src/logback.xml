<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="SWING" class="dk.dtu.sb.GUI.TextAreaAppender">
    <!-- encoders are assigned the type
         ch.qos.logback.classic.encoder.PatternLayoutEncoder by default -->
    <encoder>
      <pattern>%date{HH:mm:ss.SSS} [%thread] %-5level > %class{0}: %msg %n</pattern>
    </encoder>
  </appender>

  <root level="DEBUG">
    <appender-ref ref="SWING" />
  </root>
</configuration>