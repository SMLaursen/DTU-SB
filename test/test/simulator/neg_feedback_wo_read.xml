<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model>
    <listOfCompartments>
      <compartment id="compartment" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="mRNA" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="lacI" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="Plac" compartment="compartment" initialAmount="1" hasOnlySubstanceUnits="true"/>
      <species id="Plac_lacI" compartment="compartment" initialAmount="0" hasOnlySubstanceUnits="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="transcription" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="mRNA"/>
          <speciesReference species="Plac"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
              <cn> 0.1 </cn>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay_mRNA" reversible="false">
        <listOfReactants>
          <speciesReference species="mRNA"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
              <cn> 0.001 </cn>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="translation" reversible="false">
        <listOfReactants>
          <speciesReference species="mRNA"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lacI"/>
          <speciesReference species="mRNA"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn> 0.1 </cn>
              <ci> mRNA </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="decay_lacI" reversible="false">
        <listOfReactants>
          <speciesReference species="lacI"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
              <cn> 0.001 </cn>

          </math>
        </kineticLaw>
      </reaction>
      <reaction id="regulation" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac"/>
          <speciesReference species="lacI"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Plac_lacI"/>
          <speciesReference species="lacI"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="integer"> 1 </cn>
              <ci> Plac </ci>
              <ci> lacI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="unbinds" reversible="false">
        <listOfReactants>
          <speciesReference species="Plac_lacI"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Plac"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <cn type="e-notation"> 6 <sep/> -5 </cn>
              <ci> Plac_lacI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
